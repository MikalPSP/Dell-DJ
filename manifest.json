{
  "default_locale": "en",
  
  "app": {
      "background": {
         "scripts": [ "background.js" ]
      }
   },

   "file_handlers": {
      "audio": {
         "extensions": [ "amr", "flac", "m4a", "mp3", "oga", "ogg", "wav", "aif" ],
         "types": [ "audio/*" ]
      }
   },
   "icons": {
      "16": "assets/icons/icon_16.png",
      "19": "assets/icons/icon_19.png",
      "24": "assets/icons/icon_24.png",
      "32": "assets/icons/icon_32.png",
      "36": "assets/icons/icon_36.png",
      "48": "assets/icons/icon_48.png",
      "64": "assets/icons/icon_64.png",
      "96": "assets/icons/icon_96.png",
      "128": "assets/icons/icon_128.png",
      "256": "assets/icons/icon_256.png",
      "512": "assets/icons/icon_512.png"
   },
   
   "manifest_version": 2,
   "minimum_chrome_version": "23",
   "name": "Dell DJ",
   "description": "__MSG_appDesc__",
   "author": "Mikal Taliferro",
   
   "offline_enabled": true,
   "optional_permissions": [ "audioCapture" ],
   "permissions": [ 
     "https://www.google-analytics.com/",
     "unlimitedStorage",
     "storage", {
        "fileSystem": [ "directory", "write" ]
      }
   ],
   "short_name": "Dell DJ",
   "version": "0.1.5"
}